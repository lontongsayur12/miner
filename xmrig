# xmrig
sudo apt update
wget https://github.com/xmrig/xmrig/releases/download/v6.12.0/xmrig-6.12.0-linux-x64.tar.gz
tar -xvf xmrig-6.12.0-linux-x64.tar.gz
cd xmrig-6.12.0
./xmrig -a cryptonight-upx/2 -o stratum+tcp://cryptonight_upx.mine.zergpool.com:4457 -u XuxNNb4a3Zvb7XxgB7JECGX3y5jLKGvyxq --keepalive --donate-level 1 -p c=DASH,mc=DASH -t4
